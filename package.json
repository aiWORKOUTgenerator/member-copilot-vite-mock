{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fresh": "./scripts/dev-start.sh",
    "dev:smart": "node scripts/dev-start.js",
    "dev:kill": "lsof -ti:5177 | xargs kill -9 2>/dev/null || echo 'No process found on port 5177'",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:live": "ENABLE_LIVE_API_TESTS=true jest --config jest.config.cjs src/services/ai/external/__tests__/LiveAPIIntegration.test.ts",
    "test:unit": "jest --config jest.config.cjs --testPathIgnorePatterns=LiveAPIIntegration.test.ts",
    "test:external": "jest --config jest.config.cjs src/services/ai/external/__tests__/",
    "test:null-safety": "jest --config jest.config.cjs src/components/__tests__/null-safety.integration.test.tsx",
    "validate:null-safety": "node scripts/null-safety-validator.js",
    "setup:live-api": "./scripts/setup-live-api-tests.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
